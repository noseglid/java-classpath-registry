{
  "name": "java-classpath-registry",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Java Classpath Registry",
  "keywords": [
    "java",
    "classpath",
    "registry",
    "productivity"
  ],
  "scripts": {
    "lint": "eslint lib"
  },
  "repository": "https://github.com/noseglid/java-classpath-registry",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "bluebird": "^3.4.0",
    "glob": "^7.0.3",
    "jdjs": "^0.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "eslint": "^2.9.0",
    "eslint-config-atom-build": "^3.0.0"
  },
  "package-deps": [
    "busy"
  ],
  "consumedServices": {
    "busy": {
      "versions": {
        "^1.0.0": "consumeBusy"
      }
    }
  },
  "providedServices": {
    "java.classpath-registry": {
      "versions": {
        "1.0.0": "provide"
      }
    }
  },
  "configSchema": {
    "cacheTtl": {
      "name": "Cache Time-to-Live",
      "type": "integer",
      "default": 30,
      "minimum": 0,
      "description": "The time (in days) too keep cache of .class and .jar files."
    }
  }
}
